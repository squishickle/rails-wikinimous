<%= form_for @article do |form| %>
  <%= form.label :title, "Enter Article title" %>
  <%= form.text_field :title %>
  <%= form.label :content, "Enter content" %>
  <%= form.text_field :content %>
  <%= form.label :image, "Enter url" %>
  <%= form.text_field :image %>
  <%= form.submit %>
<% end %>
